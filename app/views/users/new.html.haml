- @misc_js << "jq('#user_login')[0].focus();"
.signupform
  %h1 Sign Up
  - form_for @user, :url => account_path do |f|
    = f.error_messages
    = render :partial => "form", :object => f
    %p
      = f.submit "Sign up"
    %p.signuplink
      Already have an account?
      = link_to "Log In", new_user_session_path
